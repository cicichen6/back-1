prefix=/usr/local
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${exec_prefix}/include

Name: libmongoc
Description: The libmongoc MongoDB client library.
Version: 1.23.2
Requires: libbson-static-1.0
Libs: -L${libdir} -lmongoc-static-1.0  -L/usr/lib/x86_64-linux-gnu -lsasl2 -L/usr/lib/x86_64-linux-gnu -lssl -L/usr/lib/x86_64-linux-gnu -lcrypto -lrt -lresolv -pthread
Cflags: -I${includedir}/libmongoc-1.0 -DMONGOC_STATIC
